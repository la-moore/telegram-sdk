{
    "name": "la-moore/telegram-sdk",
    "type": "library",
    "authors": [
        {
            "name": "Alex Moore",
            "email": "moorlex@mail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaMoore\\Tg\\": "src/",
            "LaMoore\\Tg\\Laravel\\": "laravel/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaMoore\\Tg\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^10|^11",
        "illuminate/http": "^10|^11",
        "sunrise/hydrator": "^3.10",
        "nutgram/hydrator": "^6.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10|^11",
        "orchestra/testbench": "^7|^8|^9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaMoore\\Tg\\Laravel\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "TelegramBot"
            }
        }
    }
}
